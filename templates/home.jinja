                        </div>
                        <div class="tyn-reply-group">
                            <!-- .tyn-reply-bubble -->
                            <div class="tyn-reply-bubble">
                                <div class="tyn-reply-media">
                                    <a href="${mess.content}" class="glightbox tyn-thumb" data-gallery="media-photo">
                                        <img src="${mess.content}" class="tyn-image" alt="">
                                    </a>
                                </div>
                            </div><!-- .tyn-reply-bubble -->
                        </div><!-- .tyn-reply-group -->
                    </div>
                    
                `
            }


            try {
                for (mess of messages) {
                    if (mess.kind == "image") {
                        if (mess.author != userId) {
                            document.getElementById("tynReply").insertAdjacentHTML(position, imageIncomming(mess))
                        } else {
                            document.getElementById("tynReply").insertAdjacentHTML(position, imageOutgoing(mess))
                        }
                        continue
                    }

                    if (currentRoom.type == "single" && mess.kind == "text") {
                        const aesKey = await importKeyAESFromJson(localStorage.getItem(`e2e-room-${currentRoom.id}`))
                        mess.content = arrayBufferToString(await aesCBCDecrypt(aesKey, hexToArrayBuffer(mess.content)))
                    }

                    if (mess.author != userId) {
                        document.getElementById("tynReply").insertAdjacentHTML(position, btnIncomming(mess))
                    } else {
                        document.getElementById("tynReply").insertAdjacentHTML(position, btnOutgoing(mess))
                    }
                }

            } catch (error) {
                console.log("error renderMessage", error)
            } finally {
                scrollBottom()
            }

        }

        function renderSelectedFriends() {
            document.querySelector(".create-group-chat").classList.remove("d-none");
            let html = ""
            for (let friend of selectedFriends) {
                const fullname = friend.fullname.length > 5 ? friend.fullname.substring(0, 5) : friend.fullname

                html += `
                    <div class="postion-relative">
                        <div class="tyn-media" style="background-color: #dbeafe;">
                            <img src="${friend.avatar}" alt="">
                        </div>
                        <p style="font-size: 0.75rem;">${fullname}</p>
                        <button class="btn btn-md btn-icon btn-pill btn-white shadow position-absolute btn-remove" onclick="removeSelectedFriend(${friend.userId})">
                            <svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="currentColor" class="bi" viewBox="0 0 16 16">
                                <path d="M2.146 2.854a.5.5 0 1 1 .708-.708L8 7.293l5.146-5.147a.5.5 0 0 1 .708.708L8.707 8l5.147 5.146a.5.5 0 0 1-.708.708L8 8.707l-5.146 5.147a.5.5 0 0 1-.708-.708L7.293 8z"></path>
                            </svg>
                        </button>
                    </div>
                `
            }

            if (selectedFriends.length > 0) {
                if (modeRoom == "create") {
                    html += `
                        <div class="btn-create-group" onclick="createRoom()">
                            <div class="tyn-media" style="background-color: #e2e8f0;">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-plus" viewBox="0 0 16 16">
                                    <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4"></path>
                                </svg>
                            </div>
                            <p style="font-size: 0.75rem;">Create</p>
                        </div>
                    `
                } else {
                    html += `
                        <div class="btn-create-group" onclick="joinUserRoom()">
                            <div class="tyn-media" style="background-color: #e2e8f0;">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-plus" viewBox="0 0 16 16">
                                    <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4"></path>
                                </svg>
                            </div>
                            <p style="font-size: 0.75rem;text-align: center;">Add</p>
                        </div>
                    `
                }

                document.getElementById("selected-friends").innerHTML = html
            } else {
                document.querySelector(".create-group-chat").classList.add("d-none");
            }
        }

        function addSelectedFriend({ target }, userId, fullname, avatar) {
            if (target.checked) {
                const friend = { userId, fullname, avatar }
                selectedFriends.push(friend)
            } else {
                selectedFriends = selectedFriends.filter(friend => friend.userId != userId)
            }

            renderSelectedFriends()
        }

        function removeSelectedFriend(userId) {
            selectedFriends = selectedFriends.filter(friend => friend.userId != userId)
            document.getElementById(`btn-checkbox-${userId}`).checked = false
            renderSelectedFriends()
        }

        function resetSelectedFriends() {
            selectedFriends = []
            renderSelectedFriends()
        }

        function resetChecked() {
            const checkboxes = document.querySelectorAll("#contact-search input[type='checkbox']")
            checkboxes.forEach(checkbox => {
                checkbox.checked = false
            })
        }

        document.addEventListener('DOMContentLoaded', async () => {
            const chatInput = document.querySelector('#tynChatInput-1');

            chatInput.addEventListener("keypress", function (event) {
                if (event.key === "Enter" && !event.shiftKey) {
                    event.preventDefault()
                    sendMessage()
                }
            });

            searchModal = new bootstrap.Modal(document.getElementById('newChat'), {
                keyboard: false
            })

            document.getElementById('newChat').addEventListener('hide.bs.modal', function (event) {
                modeRoom = "create"
                setTimeout(() => {
                    resetSelectedFriends()
                    resetChecked()
                }, 150)
            });

            const rooms = await getRooms()
            const { data: relationships } = await getRelationships("all")
            renderRoom("#all-chats > ul", rooms.data.data)

            friends = relationships.data
            renderSearch("#contact-search", friends)
        })

        document.getElementById("search-username").addEventListener("input", async () => {
            try {
                const username = $("#search-username").val()
                const friendFilter = friends.filter(friend => friend.username.toLowerCase().includes(username.toLowerCase()))
                renderSearch("#contact-search", friendFilter)
            } catch (error) {
                console.log(error)
            }
        })

        document.getElementById("btn-upload").addEventListener("click", () => {
            document.getElementById("upload-image").click()
        })

        document.getElementById("upload-image").addEventListener("change", async() => {
            await uploadImage()
        })
    </script>

    <script src="/static/js/libs/socket.io.min.js"></script>
    <script>
        const socket = io();
        let lastRoomId = null;

        socket.on("connect", () => {
            console.log(socket.id);
        });

        function joinRoom(roomId) {
            socket.emit('join', { room_id: roomId });
        }

        async function sendMessage() {
            const contentEl = document.getElementById("tynChatInput-1")
            if (currentRoom.type == "single") {
                const aesKey = await importKeyAESFromJson(localStorage.getItem(`e2e-room-${currentRoom.id}`))
                const content = await aesCBCEncrypt(aesKey, stringToArrayBuffer(contentEl.innerText))
                socket.emit('message', { room_id: currentRoom.id, user_id: userId, content: arrayBufferToHex(content) });
            } else {
                socket.emit('message', { room_id: currentRoom.id, user_id: userId, content: contentEl.innerText });
            }
            contentEl.innerText = ""
        }

        socket.on("image", (data) => {
            if (data.room_id == currentRoom.id) {
                renderMessage([data], false)
            }
        });

        socket.on('join', function (message) {
            console.log("join ", message)
        })

        socket.on('message', async function (message) {
            await renderMessage([message], false)
        })

        socket.on('leave', function (message) {
            console.log("leave ", message)
        })
    </script>
</body>
{% endblock %}