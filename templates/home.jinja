        async function hidenChatContainerAndLoadRooms() {
            const chatMain = document.getElementById("tynMain")
            chatMain.classList.add("v-hide")
            const { data: rooms } = await getRooms()
            renderRoom("#all-chats > ul", rooms.data)
        }

        async function renderProfileAndChat(roomId) {
            try {
                let roomName = ""
                const { data: room } = await getRoom(roomId)
                const chatMain = document.getElementById("tynMain")
                if (!room.data) {
                    await hidenChatContainerAndLoadRooms()
                    return
                }
                currentRoom = room.data

                renderNameAndAvatarRoom(room.data)
                renderManageRoom(room.data)
                if (chatMain.classList.contains("v-hide")) {
                    chatMain.classList.remove("v-hide");
                }

                if (localStorage.getItem(`e2e-room-${roomId}`) == null && room.data.type == "single") {
                    const friend = room.data.friends.find(friend => friend.user_id != userId)
                    await createSharedKey(roomId, friend.public_key)
                }

                const { data: messages } = await getHistory(roomId)
                await renderMessage(messages.data)
                joinRoom(roomId)
                TynApp.Plugins.lightbox();
            } catch (error) {
                console.log(error)
                console.log('Stack trace:', error.stack);
            }
        }

