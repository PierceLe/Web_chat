    <script>
        let searchModal = null
        let currentRoom = null
        let friends = []
        let selectedFriends = []
        let modeRoom = "create"

        async function getRooms() {
            return await axios.get(`/api/rooms`, {}, {
                headers: {
                    "X-CSRF-TOKEN": getCookie("csrf_access_token")
                }
            })
        }

        async function getRoom(roomId) {
            return await axios.get(`/api/rooms/${roomId}`, {}, {
                headers: {
                    "X-CSRF-TOKEN": getCookie("csrf_access_token")
                }
            })
        }

        async function uploadImage(){
            const file = document.getElementById("upload-image").files[0]
            const formData = new FormData()
            formData.append("image", file)
            return await axios.post(`/api/rooms/upload-image/${currentRoom.id}`, formData, {
                headers: {
                    "X-CSRF-TOKEN": getCookie("csrf_access_token"),
                    "Content-Type": "multipart/form-data"
                }
            })
        }

        async function getHistory(roomId) {
            return await axios.get(`/api/rooms/history/${roomId}`, {}, {
                headers: {
                    "X-CSRF-TOKEN": getCookie("csrf_access_token")
                }
            })
        }

        async function createRoom() {
            try {
                const { data } = await axios.post(`/api/rooms`, {
                    users: selectedFriends.map(friend => friend.userId),
                    roomName: selectedFriends.map(friend => friend.fullname).join(", ")
                }, {
                    headers: {
                        "X-CSRF-TOKEN": getCookie("csrf_access_token")
                    }
                })

                searchModal.hide()
                resetSelectedFriends()
                resetChecked()
                const rooms = await getRooms()
                renderRoom("#all-chats > ul", rooms.data.data)
            } catch (error) {
                console.log(error)
            }
        }

        async function joinUserRoom() {
            try {
                const { data } = await axios.post(`/api/rooms/join-room/${currentRoom.id}`, {
                    users: selectedFriends.map(friend => friend.userId),
                }, {
                    headers: {
                        "X-CSRF-TOKEN": getCookie("csrf_access_token")
                    }
                })

                searchModal.hide()
                const { data: room } = await getRoom(currentRoom.id)
                renderManageRoom(room.data)
            } catch (error) {
                console.log(error)
            }
        }

        async function leaveRoom(userId1) {
            try {
                const { data } = await axios.post(`/api/rooms/leave-room/${currentRoom.id}`, {
                    userId: userId1
                }, {
                    headers: {
                        "X-CSRF-TOKEN": getCookie("csrf_access_token")
                    }
                })

                if (userId == userId1) {
                    await hidenChatContainerAndLoadRooms()
                }
                else {
                    renderProfileAndChat(currentRoom.id)
                }
            } catch (error) {
                console.log(error)
            }
        }

        async function searchFriend(username) {
            return await axios.get(`/api/users/${username}`, {}, {
                headers: {
                    "X-CSRF-TOKEN": getCookie("csrf_access_token")
                }
            })
        }

        async function createSharedKey(roomId, publicKeyJson) {
            const publicKeyFriend = await importKeyFromJson(publicKeyJson)
            const privateKey = await importKeyFromJson(localStorage.getItem("e2e-prkey"))
            const sharedSecret = await getSharedSecret(privateKey, publicKeyFriend)
            const keyAES = await aesCBCGenerateKey(sharedSecret)
            localStorage.setItem(`e2e-room-${roomId}`, JSON.stringify(keyAES))
        }

        async function deleteRoom(group_id) {
            try {
                const res = await axios.delete(`/api/rooms/${group_id}`, {
                    headers: {
                        "X-CSRF-TOKEN": getCookie("csrf_access_token")
                    }
                })

                await hidenChatContainerAndLoadRooms()
                localStorage.removeItem(`e2e-room-${group_id}`)
                alert_success("Delete successfully!")
            } catch (error) {
                console.log(error)
            }
        }
    </script>
